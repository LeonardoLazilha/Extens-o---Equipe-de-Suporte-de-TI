{
    "manifest_version": 3,
    "name": "Extensão CSTI",
    "version": "1.0",
    "description": "Uma extensão simples para copiar textos.",
    "permissions": ["clipboardWrite"],
    "action": {
      "default_popup": "index.html"
    },
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "128": "icons/icon128.png"
    }    
  }
  